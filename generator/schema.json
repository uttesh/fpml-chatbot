                }
            },
            "xsd:complexContent": {
                "xsd:extension": {
                    "@base": "DataDocument",
                    "xsd:sequence": {
                        "xsd:element": [
                            {
                                "@ref": "market",
                                "@minOccurs": "0",
                                "@maxOccurs": "unbounded"
                            },
                            {
